{
  "extends": [
    "angular",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "rules": {
    "angular/avoid-scope-typos": 0,
    "angular/module-getter": 2,
    "angular/module-setter": 2,
    "angular/no-private-call": 2,
    "angular/component-limit": 2,
    "angular/controller-as-route": 2,
    "angular/controller-as-vm": 1,
    "angular/controller-as": 2,
    "angular/deferred": 1,
    "angular/di-unused": 2,
    "angular/directive-restrict": 2,
    "angular/empty-controller": 2,
    "angular/no-controller": 0,
    "angular/no-inline-template": [
      2,
      {
        "allowSimple": false
      }
    ],
    "angular/no-run-logic": 0,
    "angular/no-services": 2,
    "angular/on-watch": 2,
    "angular/prefer-component": 0,
    "angular/no-cookiestore": 2,
    "angular/no-directive-replace": 2,
    "angular/no-http-callback": 2,
    "angular/component-name": [
      2,
      "/^coppel/"
    ],
    "angular/constant-name": 0,
    "angular/controller-name": 0,
    "angular/factory-name": 0,
    "angular/directive-name": [
      2,
      "/^coppel/"
    ],
    "angular/file-name": [
      2,
      {
        "typeSeparator": "dot"
      }
    ],
    "angular/filter-name": [
      2,
      "/^coppel/"
    ],
    "angular/module-name": 0,
    "angular/di-order": 1,
    "angular/di": [
      2,
      "array"
    ],
    "angular/dumb-inject": 0,
    "angular/function-type": [
      1,
      "anonymous"
    ],
    "angular/module-dependency-order": 0,
    "angular/no-service-method": 0,
    "angular/one-dependency-per-line": 2,
    "angular/rest-service": 0,
    "angular/watchers-execution": 0,
    "angular/no-angular-mock": 0,
    "angular/angularelement": 2,
    "angular/definedundefined": 1,
    "angular/document-service": 2,
    "angular/foreach": 2,
    "angular/interval-service": 2,
    "angular/json-functions": 2,
    "angular/log": 2,
    "angular/no-jquery-angularelement": 2,
    "angular/timeout-service": 2,
    "angular/typecheck-array": 2,
    "angular/typecheck-date": 2,
    "angular/typecheck-function": 2,
    "angular/typecheck-number": 2,
    "angular/typecheck-object": 2,
    "angular/typecheck-string": 2,
    "angular/window-service": 2,
    "angular/on-destroy": 2,
    "no-cond-assign": 2,
    "no-compare-neg-zero": 1,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 1,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 1,
    "no-extra-semi": 2,
    "no-func-assign": 1,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-obj-calls": 2,
    "no-prototype-builtins": 1,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-template-curly-in-string": 0,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "accessor-pairs": 2,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "class-methods-use-this": 0,
    "complexity": 0,
    "consistent-return": 2,
    "curly": 2,
    "default-case": 2,
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 0,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 0,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-empty-function": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-fallthrough": 0,
    "no-floating-decimal": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-restricted-properties": 2,
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": 2,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "prefer-promise-reject-errors": 2,
    "radix": 2,
    "require-await": 2,
    "vars-on-top": 2,
    "yoda": 2,
    "init-declarations": [
      2,
      "always"
    ],
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undef-init": 0,
    "no-undefined": 0,
    "no-unused-vars": [
      2,
      {
        "varsIgnorePattern": "[iI]gnored",
        "argsIgnorePattern": "^_"
      }
    ],
    "no-use-before-define": 2,
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "capitalized-comments": [
      2,
      "always"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": 0,
    "computed-property-spacing": 2,
    "consistent-this": 0,
    "eol-last": 2,
    "func-call-spacing": 2,
    "func-name-matching": 0,
    "func-names": 0,
    "func-style": [
      2,
      "expression"
    ],
    "id-blacklist": 0,
    "id-length": 0,
    "id-match": 0,
    "indent": [
      2,
      4
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false
      }
    ],
    "line-comment-position": [
      2,
      {
        "position": "above"
      }
    ],
    "linebreak-style": 0,
    "lines-around-comment": [
      2,
      {
        "beforeBlockComment": true
      }
    ],
    "lines-around-directive": 0,
    "max-depth": [
      2,
      6
    ],
    "max-len": [
      2,
      {
        "code": 120,
        "tabWidth": 4,
        "comments": 120,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "max-lines": [
      "error",
      {
        "max": 500,
        "skipComments": true,
        "skipBlankLines": true
      }
    ],
    "max-nested-callbacks": [
      2,
      4
    ],
    "max-params": [
      2,
      20
    ],
    "max-statements": [
      0,
      10
    ],
    "max-statements-per-line": [
      2,
      {
        "max": 1
      }
    ],
    "multiline-ternary": [
      2,
      "never"
    ],
    "new-cap": [
      0,
      {
        "newIsCap": true
      }
    ],
    "new-parens": 0,
    "newline-after-var": [
      2,
      "always"
    ],
    "newline-before-return": 2,
    "newline-per-chained-call": [
      2,
      {
        "ignoreChainWithDepth": 2
      }
    ],
    "no-array-constructor": 0,
    "no-bitwise": 0,
    "no-continue": 2,
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-mixed-operators": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-assign": 2,
    "no-multiple-empty-lines": 2,
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-plusplus": [
      0,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-tabs": 1,
    "no-ternary": 0,
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": true
      }
    ],
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "object-curly-newline": 0,
    "object-property-newline": 2,
    "one-var": 0,
    "operator-assignment": 2,
    "semi": 2,
    "semi-spacing": 2,
    "sort-keys": 1,
    "sort-vars": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": 0,
    "space-in-parens": 0,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": [
      1,
      "always"
    ],
    "template-tag-spacing": 0,
    "unicode-bom": 0,
    "wrap-regex": 2
  },
  "env": {
    "browser": true
  },
  "globals": {
    "module": true,
    "inject": true,
    "require": true,
    "angular": true,
    "ApplicationConfiguration": true
  }
}